<idea-plugin>

    <extensions defaultExtensionNs="spring.assistant.initializr">
        <moduleBuilderPostProcessor
                implementation="org.jetbrains.plugins.template.maven.MavenModuleBuilderPostProcessor"/>
    </extensions>


    <project-components>
        <component>
            <interface-class>
                org.jetbrains.plugins.template.maven.MavenReIndexingDependencyChangeSubscriber
            </interface-class>
            <implementation-class>
                org.jetbrains.plugins.template.maven.MavenReIndexingDependencyChangeSubscriberImpl
            </implementation-class>
        </component>
    </project-components>


</idea-plugin>
